{
	"description": "Display 360 degree panoramic media from brightcove",
	"origamiType": "module",
	"origamiVersion": 1,
	"support": "https://github.com/Financial-Times/o-three-sixty/issues",
	"supportStatus": "active",
	"ci": {
		"circle": "https://circleci.com/api/v1/project/Financial-Times/o-three-sixty"
	},
	"browserFeatures": {
		"required": [
			"Element.prototype.matches"
		]
	},
	"demosDefaults": {
		"js": "./demos/src/demo.js",
		"sass": "./main.scss"
	},
	"demos": [
		{
			"name": "o-video",
			"template": "demos/src/o-video.mustache",
			"expanded": true,
			"description": "Showing 360 video content from o-video"
		},
		{
			"name": "img",
			"template": "demos/src/img.mustache",
			"expanded": true,
			"description": "Showing 360 a 360 image from an image element."
		}
	]
}
